            </resource>
            <resource id="roles" path="roles"> &authHeader; <method
                    href="#createRole"/>
                <method href="#listRoles"/>
                <resource id="roleID" path="{role_id}">
                    <param name="role_id" style="template"
                        type="capi:uuid" required="true">
                        <wadl:doc
                            xmlns="http://docbook.org/ns/docbook">The
                            role ID.</wadl:doc>
                    </param>
                    <method href="#getRole"/>
                    <method href="#updateRole"/>
                    <method href="#deleteRole"/>
                    <resource id="roleIDUsers" path="users">
                        &listRoleUsersParams; <method
                            href="#listRoleUsers"/>
                    </resource>
                </resource>
            </resource>
            <resource id="policies" path="policies"> &authHeader;
                    <method href="#createPolicy"/>
                <method href="#listPolicies"/>
                <resource id="policyID" path="{policy_id}">
                    <param name="policy_id" style="template"
                        type="capi:uuid" required="true">
                        <wadl:doc
                            xmlns="http://docbook.org/ns/docbook">The
                            policy ID.</wadl:doc>
                    </param>
                    <method href="#getPolicy"/>
                    <method href="#updatePolicy"/>
                    <method href="#deletePolicy"/>
                </resource>
            </resource>
        </resource>
    </resources>

