                    <resource id="domainIDGroupIDRoles"
                        path="groups/{group_id}/roles">
                        <param name="group_id" style="template"
                            type="capi:uuid" required="true">
                            <wadl:doc
                                xmlns="http://docbook.org/ns/docbook"
